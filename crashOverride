  const last = l.charAt(0).toUpperCase();

  const firstAlias = firstName[first];
  const lastAlias = surname[last];

  if (!firstAlias || !lastAlias) {
    return 'Your name must start with a letter from A - Z.';
  }
  return firstAlias + ' ' + lastAlias;
}
