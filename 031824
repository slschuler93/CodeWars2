//multiply the number 7 kyu
function multiply(number) {
  return number * (5 ** String(Math.abs(number)).length);
}

//remove exclamation marks 8 kyu
function removeExclamationMarks(s) {
  return s.replace(/!/gi, '');
}

//welcome! 8 kyu
